{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/login": {
            "post": {
                "description": "就是用户登录呗",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "用户登录",
                "parameters": [
                    {
                        "description": "用户登录输入",
                        "name": "struct",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.UserLoginInput"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "success",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/middleware.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/dto.UserLoginOutput"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/middleware.Response"
                        }
                    }
                }
            }
        },
        "/resource/cpu": {
            "get": {
                "description": "获取CPU基本的状态",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "resource"
                ],
                "summary": "获取CPU基本的状态",
                "responses": {
                    "200": {
                        "description": "success",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/middleware.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/dto.CPUList"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/middleware.Response"
                        }
                    }
                }
            }
        },
        "/resource/gpu": {
            "get": {
                "description": "获取GPU基本的状态包含个数、占用率等",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "resource"
                ],
                "summary": "获取GPU基本的状态",
                "responses": {
                    "200": {
                        "description": "success",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/middleware.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "$ref": "#/definitions/dto.GPUList"
                                        }
                                    }
                                }
                            ]
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/middleware.Response"
                        }
                    }
                }
            }
        },
        "/user/logout": {
            "get": {
                "description": "就是用户登出呗",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "用户登出",
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/middleware.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/middleware.Response"
                        }
                    }
                }
            }
        },
        "/user/register": {
            "post": {
                "description": "用户注册",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "user"
                ],
                "summary": "用户注册（仅限管理员）",
                "parameters": [
                    {
                        "description": "新建用户输入",
                        "name": "struct",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/dto.NewUserInput"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "success",
                        "schema": {
                            "$ref": "#/definitions/middleware.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/middleware.Response"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "dto.CPUList": {
            "type": "object",
            "properties": {
                "count": {
                    "type": "integer"
                },
                "cpuinfo": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dto.CPUStatus"
                    }
                }
            }
        },
        "dto.CPUStatus": {
            "type": "object",
            "properties": {
                "cores": {
                    "type": "integer"
                },
                "maxTurbo": {
                    "type": "number"
                },
                "modelName": {
                    "type": "string"
                },
                "vendorID": {
                    "type": "string"
                }
            }
        },
        "dto.GPUList": {
            "type": "object",
            "properties": {
                "count": {
                    "type": "integer"
                },
                "cudadriverVersion": {
                    "type": "string"
                },
                "driverVersion": {
                    "type": "string"
                },
                "gpuinfo": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dto.GPUStatus"
                    }
                }
            }
        },
        "dto.GPUStatus": {
            "type": "object",
            "properties": {
                "deviceName": {
                    "type": "string"
                },
                "memoryTotal": {
                    "type": "integer"
                },
                "memoryUsed": {
                    "type": "integer"
                },
                "powerDefaultLimit": {
                    "type": "integer"
                },
                "powerUsage": {
                    "type": "integer"
                },
                "processInfo": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/dto.ProcessInfoCombined"
                    }
                },
                "uuid": {
                    "type": "string"
                }
            }
        },
        "dto.NewUserInput": {
            "type": "object",
            "required": [
                "email",
                "raw_password",
                "user_role",
                "username"
            ],
            "properties": {
                "email": {
                    "type": "string"
                },
                "raw_password": {
                    "type": "string"
                },
                "user_role": {
                    "type": "integer"
                },
                "username": {
                    "type": "string"
                }
            }
        },
        "dto.ProcessInfoCombined": {
            "type": "object",
            "properties": {
                "commandLine": {
                    "type": "string"
                },
                "pid": {
                    "type": "integer"
                },
                "usedGpuMemory": {
                    "type": "integer"
                },
                "user": {
                    "type": "string"
                }
            }
        },
        "dto.UserLoginInput": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "description": "管理员密码",
                    "type": "string",
                    "example": "12345678"
                },
                "username": {
                    "description": "管理员用户名",
                    "type": "string",
                    "example": "joexu01"
                }
            }
        },
        "dto.UserLoginOutput": {
            "type": "object",
            "properties": {
                "token": {
                    "description": "返回的Token",
                    "type": "string",
                    "example": "token"
                }
            }
        },
        "middleware.Response": {
            "type": "object",
            "properties": {
                "data": {},
                "err_msg": {
                    "type": "string"
                },
                "errno": {
                    "type": "integer"
                },
                "stack": {},
                "trace_id": {}
            }
        }
    }
}